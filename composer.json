{
  "name": "mammatus/groups",
  "description": "\ud83d\udc69\u200d\ud83d\udc69\u200d\ud83d\udc67\u200d\ud83d\udc66 All groups related tooling",
  "license": "MIT",
  "type": "composer-plugin",
  "require": {
    "php": "^8.4",
    "ext-json": "^8.4",
    "composer-plugin-api": "^2.0",
    "mammatus/life-cycle-events": "^2.3.0",
    "psr/container": "^1 || ^2 || ^3",
    "psr/log": "^3.0.2",
    "roave/better-reflection": "^6.69.0",
    "wyrihaximus/broadcast-contracts": "^1.3.0",
    "wyrihaximus/generative-composer-plugin-tooling": "^1.4.0"
  },
  "require-dev": {
    "colinodell/psr-testlogger": "^1.3.1",
    "pimple/pimple": "^3.6.1",
    "shipmonk/dead-code-detector": "^0.14.1",
    "wyrihaximus/broadcast": "^2.6",
    "wyrihaximus/makefiles": "^0.9.0",
    "wyrihaximus/test-utilities": "^12.0.0"
  },
  "suggest": {
    "shipmonk/dead-code-detector": "This package supports ^0.14.1 by hooking in and marking all listeners as alive"
  },
  "autoload": {
    "psr-4": {
      "Mammatus\\DevApp\\Groups\\": "etc/dev-app/",
      "Mammatus\\Groups\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mammatus\\Tests\\Groups\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupol/composer-packages": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "mammatus/cron": true,
      "mindplay/composer-locator": true,
      "phpstan/extension-installer": true,
      "wyrihaximus/broadcast": true,
      "wyrihaximus/makefiles": true,
      "wyrihaximus/test-utilities": true
    },
    "platform": {
      "php": "8.4.13"
    },
    "sort-packages": true
  },
  "extra": {
    "class": "Mammatus\\Groups\\Composer\\Installer",
    "mammatus": {
      "has-groups": true
    },
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    },
    "wyrihaximus": {
      "broadcast": {
        "has-listeners": true
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "make on-install-or-update || true"
    ],
    "post-update-cmd": [
      "make on-install-or-update || true"
    ],
    "pre-autoload-dump": [
      "Mammatus\\Groups\\Composer\\Installer::findGroups"
    ],
    "generate-config": [
      "Mammatus\\Groups\\Composer\\Installer::findGroups"
    ]
  }
}
